<!-- BEGIN: main -->

<div class="viewlist">
	<!-- BEGIN: loop -->
	<div class="panel panel-default">
		<div class="panel-body">
			<div class="pull-left">
				<a href="{DATA.url_view}" title="{DATA.title}">
				<!-- BEGIN: icon -->
				<em class="fa fa-code fa-5x list-image">&nbsp;</em>
				<!-- END: icon -->
				<!-- BEGIN: image -->
				<img src="{DATA.image}" class="img-thumbnail list-image" width="90" />
				<!-- END: image -->
				</a>
			</div>
			<h2><a href="{DATA.url_view}" title="{DATA.title}">{DATA.title}</a></h2>
			<em class="help-block">{DATA.description}</em>
		</div>
	</div>
	<!-- END: loop -->
	
	<!-- BEGIN: page -->
	<div class="text-center">{PAGE}</div>
	<!-- END: page -->
</div>

<!-- END: main -->